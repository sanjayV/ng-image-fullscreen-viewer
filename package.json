    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "core-js": "^3.4.5",
    "rxjs": "^6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2",
    "rxjs-compat": "^6.0.0-rc.0"
  },
